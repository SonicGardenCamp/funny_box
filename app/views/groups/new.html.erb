<h1グループ作成</h1>

<div>
  <div>
    <%= form_with(model: @group) do |f| %>

      <%= f.label :name, "グループ名" %>
      <%= f.text_field :name %>

      <%= f.label :description, "グループの説明" %>
      <%= f.text_area :description %>
      
      <%= f.submit "新規登録" %>
    <% end %>
  </div>
</div>

<%= link_to "一覧へ戻る", groups_path %>